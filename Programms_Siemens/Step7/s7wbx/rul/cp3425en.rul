	      # Identifier für Eingangs Modul xxxx:Vo 14.07.95 b10 instead b01
	      ExtIoTypeKey$KEY     IC|0b10|2|2|2

	      # Länge des Eingangs Modul
	      IoLen                IM|0b100000|0|6|6
	      *Length of the I/O area

	      # Ausrichtung des Eingangs Modul
	      Alignment            EM|Byte|1|Byte(0)|Word(1)
	      *Granularity of the I/O area

	      # Konsistenz des Eingangs Modul
	      Consistency          EM|Area|1|Value(0)|Area(1)
	      *Consistency of the I/O area

	      }

	    # Ausgangs Modul
	    OutDescr$NAM           _H||||
	    OutDescr
	      { *contains the description of an ouput module

	      # Identifier für Ausgangs Modul xxxx:Vo 14.07.95 b01 instead b10
	      ExtIoTypeKey$KEY     IC|0b01|2|2|2

	      # Länge des Ausgangs Moduls
	      IoLen                IM|0b100000|0|6|6
	      *Length of the I/O area

	      # Ausrichtung des Ausgangs Moduls
	      Alignment            EM|Byte|1|Byte(0)|Word(1)
	      *Granularity of the I/O area

	      # Konsistenz des Ausgangs Moduls
	      Consistency          EM|Area|1|Value(0)|Area(1)
	      *Consistency of the I/O area

	      }

	    # Ein- Ausgangs Modul
	    InOutDescr$NAM         _H||||
	    InOutDescr
	      { *contains the description of an input/output module

	      # Identifier für Ein- Ausgangs Modul
	      ExtIoTypeKey$KEY     IC|0b11|2|2|2

	      # Länge des Ausgangs Moduls
	      OutLength            IM|0b100000|0|6|6
	      *Length of the output area

	      # Ausrichtung des Ausgangs Moduls
	      OutAlignment         EM|Byte|1|Byte(0)|Word(1)
	      *Granularity of the output I/O area

	      # Konsistenz des Ausgangs Moduls
	      OutConsistency       EM|Area|1|Value(0)|Area(1)
	      *Consistency of the output I/O area

	      # Länge des Eingangs Moduls
	      InLength             IM|0b100000|0|6|6
	      *Length of the input I/O area

	      # Ausrichtung des Eingangs Moduls
	      InAlignment          EM|Byte|1|Byte(0)|Word(1)
	      *Granularity of the input I/O area

	      # Konsistenz des Eingangs Moduls
	      InConsistency        EM|Area|1|Value(0)|Area(1)
	      *Consistency of the input I/O area
	      }

	    # optionale Hersteller spezifische Daten
	    ManufData                               
	      {
	      # Hersteller spezifische Daten
	      ManufactData         HO||0|14|0
	      *Vendor data
	      }
	    }
	  }
	}
      }
    }
  Block2                           HO||0|65535|0
  Block3                           HO||0|65535|0
  Block4                           HO||0|65535|0
  }

 
