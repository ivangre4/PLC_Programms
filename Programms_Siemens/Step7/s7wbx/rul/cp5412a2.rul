  DpSDB$LNG                                DM||0|1000000|  # Laenge des SDB
  }
# **************************************************************************

DpSDB$NAM               _O||||                  # DP SDB
DpSDB
  { * This parameter field contains the DP configuration

  SDBParameterblock$NAM   _M||||
  SDBParameterblock
         {
         #
         # DP Management Parameterblock
         #
         # Parameterblock 1
         PblkDpMgt                       BC|1|||
         # Version  0
         PblkVersion                     BC|0|||
         # Parameterblock Length
         #DpManagement$LNG      WM||||
         DpManagementLEN                 WC|37|||                

         # Amount Poll Cycles to reset Station List
         CtrPollToResetStatLst BM|1|0|255|1
         * Number of polling cycles until the station list is reset

         # ueberwacht Datentransfer und Benutzer Interface ( 6 * Watchdog [ 10 ms])
         DataControlTime       WM|0|0|0xffff|1
         * Monitors the DP data transfer and the user interface

         # Master Poll Timeout
         MasterPollTimeout     WM|1000|0|0xffff|1
         * Minimum polling cycle time of the master

         # Poll_Timeout fuer den Master 2
         Master2PollTimeout    WM|200|0|0xffff|1
         * Polling timeout of the MASTER class 2

         # Name of corresponding Master GSD File
         MasterGsdFile         AM|"CP5412A2.GSD"|1|14|14
         * Name of the master GSD file

         # Watchdog Timeout für die Oberfläche
         reservedWdTimeout     WM|400|10|0xfe01|1
         * Watchdog timeout, only used by the user interface

         # Verhalten bei Übertragung des DP-Ausgangsbereichs
         ClearDP               EM|FALSE|1|FALSE(0b0)|TRUE(0b1)
         * Behavior during transfer of the DP output area

         # Ist ein Zyklisches Steuertelegramm gefordert
         zyklGlobalCtrlCommand           EM|NONE|2|NONE(0b00)|SYNC(0b01)|FREEZE(0b10)|SYNC_FREEZE(0b11)
         * Which cyclic control frame is requested

         # Soll die Watchdog Zeit automatisch berechnet werden
         bWdDefault                      EM|TRUE|1|FALSE(0b0)|TRUE(0b1)
         * Will the watchdog time be calculated automatically

         # Reserve
         ReservedDp                      IC|0b0000|4|4|4

         # Gruppen Identifikation für zyklisches Steuertelegramm
         zyklGlobalCtrlGrpIdent          BM|0|0|255|1
         * Group identification for cyclic control frame

         # maximaler Wert des Min Slave Intervall in der Datenbasis
         maxMinSlvIntervall              WM|0|0|0xffff|1
         * Maximum value of the min. slave interval in the database [ ms]

         # DP Bremszeit
         DpDelayTime                     WM|0|0|0xffff|1
         * Time for delaying DP [ ms]

         # Amount Not Norm Slaves ( Maximum 125 )
         NotNormSlave$CNT      WO||0|125|1

         # Amount edited Slave ( Maximum 125 )
         NormSlave$CNT         WO||0|125|1              

         #
         # Ende DP Management Parameterblock
         #



         # Norm Slave Description
         NormSlave$NAM         _A||||                  
         NormSlave
                {  * Contains the complete description of standrd slave
                # Parameterblock
                # ********
                PblkDpMgt           BC|2|||                 # Parameterblock 2
                PblkVersion         BC|0|||                 # Version  0
                NormSlave$LNG       WM||||                  # Parameterblock Length
                # ********

                # Slave Header
                SlvHeader                                   
                  {

                  # Length of Slave Header
                  SlvHeader$LNG       WM||||

                  # Slave SINEC L2 Adress
                  SlvAdr              BM|1|1|126|1
                  * SINEC L2 station address of the slave          
                                                                                                                                
                  # Identifikation fuer einen Norm Slave
                  NormSlaveKey$KEY    BC|0|||

                  # Name of Slave
                  SlvName             AM|"                        "|1|24|24
                  * Name of the slave

                  # Name of corresponding GSD File
                  GsdFile             AM|"              "|1|14|14
                  * Name of the corresponding GSD file              

                  # Min Slave Intervall
                  MinSlvIntervall     WM|20|0|0xffff|1
                  * Minimum slave interval

                  # Aktiver oder passives Slave
                  SlvActivate         EM|Activated|1|Activated(0b1)|NotActivated(0b0)
                  * Will the slave be polled by the master

       # DPV1-Master quits alarms via SAP 51 (FALSE - Defaul) or via SAP 50 (TRUE)
                  DPV1ExtraAlarmSAP EM|FALSE|1|FALSE(0b0)|TRUE(0b1)
                  * DPV1-Master quiitiert Alarme über den SAP 51 (FALSE - Default) oder SAP 50 (TRUE)

                  # Slave CFG data of DPV1?
                  DPV1DataTypes               EM|FALSE|1|FALSE(0b0)|TRUE(0b1)
                  * Slave CFG data of DPV1?

                  # DPV1 functionality
                  DPV1Supported               EM|FALSE|1|FALSE(0b0)|TRUE(0b1)
                  * Slave functionally of DPV1

                  # DPV1 Alarm supported
                  DPV1AlarmSupported          EM|FALSE|1|FALSE(0b0)|TRUE(0b1)
                  * Slave Diag format of DPV1

                  # Failsafe mode
                  FailSafe                    EM|FALSE|1|FALSE(0b0)|TRUE(0b1)
                  * Slave receives zero or no data in CLEAR mode

                  # Slave receives new PRM data
                  NewPrm                      EM|FALSE|1|FALSE(0b0)|TRUE(0b1)
                  * Slave receives data or new PRM data

                  # Slave is activated
                  Active                      EM|TRUE|1|FALSE(0b0)|TRUE(0b1)
                  * Slave is activated or deactivated

				  SubScribBlk$NAM						_O||||
  			      SubScribBlk
			      {
			        SubScribBlkId						BC|10|||          # Slavequerverkehr (Publisher/Subscriber) Block
			        SubScribFeatures					WM|0|0|0xffff|1   # default : Alle optionale Blöcke aus
			                                                              # Bit 0=1 : Publisher/Subscriber Version 2.0
			        SubScribBlk$LNG                     WM||||
			      
			        SubScribAddData$NAM					_O||||
			        SubScribAddData
			        {
			          SubScribAddDataId                 BC|0|||           
			          SubScribAddData$LNG               WM||||
			        
			          SlvPublisher                      BM|0|0|1|1          # Slave is publisher  : 0=off 1=on
			          SlvSubscriber                     BM|0|0|1|1          # Slave is subscriber : 0=off 1=on
			          UsedSap							WM|0|0|2|1          # 0 : Default SAP
			                                                                # 1 : in case of ddb_sap_60_activation
			                                                                # 2 : in case of ddb_sap_57_activation
			          DDBBlock1$NAM                      _H||||
					  DDBBlock1
					  {			                                                                
				          PrmChannel                    DC|0|0|0|0          # 0 : parameter are sent via DDB-Prm-Channel
			              DDBList$LNG                   DM|0|0|0xffffffff|1 
				          DDBList						HO||1|0xffff|0
				      }
	
			          DDBBlock2$NAM                      _H||||
					  DDBBlock2
					  {			                                                                
				          PrmChannel                    DM||1|0xffffffff|1  # 0 : parameter are sent via DDB-Prm-Channel
			                                                                # n : parameter are sent via Prm_data, 
			                                                                #     n is index within Prm_data (begin of DDB-Data)
				                                                                
			              DDBLen                        DM|0|0|0xffffffff|1 
				      }
			        }
			      }

				  Dpv1Blk$NAM							_O||||
  			      Dpv1Blk
			      {
			        Dpv1BlkId						    BC|20|||          # DPV1 Block
			        Dpv1ProfilesFeatures				WM|0|0|0xffff|1   # default : Alle optionalen Blöcke aus
			                                                              # Bit 0=1 : DPV1 Version 2.0
			        Dpv1Blk$LNG                         WM||||
			      
			        Dpv1AddData$NAM                     _O||||
			        Dpv1AddData
			        {
			          Dpv1AddDataId                     BC|0|||           
			          Dpv1AddData$LNG                   WM||||
			        
			          S7ModeEnable                      BM|0|0|1|1          # Slave wird im S7-Modus betrieben
			          AsicType                          BM|0|0|255|1        # 0 : Default, 1 : LSPM2
			          MaxDiagDataLen                    BM|0|0|255|1        # 
			          MaxAlarmLen                       BM|4|4|64|1       # Maximum length of DPV1-Alarm within Slave-Diag-PDU
			          MaxChannelDataLength              BM|4|4|244|1        # Maximum Size of MSAC1-PDU
			          DiagUpdDelay                      BM|0|0|255|1        # Counter for Slaves with reduced performance
			          AlarmMode                         BM|0|0|7|1          # Maximum number of possible active alarms
			                                                                # 0:one of each type 
			                                                                # 1:--> 2 in total
			                                                                # 2:4; 3:8; 4:12; 5:16; 6:24; 7:32
			          AddSlFlag                         BM|0|0|255|1        # 
			          C1ResponseTimeout                 BM|0|0|255|1        #
			          C1ServSupported                   WM|0|0|0xffff|1     # Bit 0 : ReadWrite supported
			                                                                # Bit 1 : ReadWrite required
			                                                                # Bit 2 : DiagAlarm supported
			                                                                # Bit 3 : DiagAlarm required
			                                                                # Bit 4 : ProcessAlarm supported
			                                                                # Bit 5 : ProcessAlarm required
			                                                                # Bit 6 : PullPlugAlarm supported
			                                                                # Bit 7 : PullPlugAlarm required
