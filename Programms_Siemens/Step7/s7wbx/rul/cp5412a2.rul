# ****************************************************************************
# SDB for System-Parameter
# ****************************************************************************
# **************************************************************************
# Header
# **************************************************************************
SystemSDBHeader
{
  SystemSDBBlockId       BC|121|||       # Blockidentifier
  SystemSDB$LNG          DM||0|1000000|  # Laenge des SDB
}

SystemSDB$NAM            _M||||          # SDB for System-Parameter
SystemSDB
{   * Important! Do not modify the system SDB field (bus parameters)
    *            using a text editor !!!
    * System Information
         * contains all the information for the module to start up

  SDBParameterblock$NAM  _M||||
  SDBParameterblock
  {
    BusParHeader                         # Busparameter Header
    {
      # Parameterblock
      # ********
      PblkNo1             BC|1|||        # Parameterblock Number
      PblkVersion1        BC|0|||        # Version  0
      BusParHeader$LNG    WM||||         # Parameterblock Length
      # ********

      # Are Busparameter local edited ?
      LocalEdit                               EM|TRUE|8|FALSE(0)|TRUE(1)
      * The bus parameters were edited locally

      # Maximum Amount Masters
      MaxMaster<MaxMaster>                    BM|126|1|126|1
      * Number of master modules in the network

      # Busparameter Calculation Algorithm
          BusParCalculate<BusParCalculate> EM|Fast|8|Smart<universell>(1)
                                              |Fast<standard>(2)
                                              |DpMono<dpmono>(3)
                                              |UserDefined<benutzerdefiniert>(0)
      * Algorithm for calculating the bus parameters

      # logical Subnet Profile
          ProfileType<ProfileType>                  BM|0|0|255|1
      * Type of subnet

      # maximale Anzahl Slave
          MaxSlave<MaxSlave>               BM|10|0|126|1
      * Number of DP slaves in the network

    }

    # SINEC Busparameter Block
    BusParBlk                                 
    {
      # Parameterblock
      # ********
      PblkNo2                  BC|2|||             # Parameterblock Number
      PblkVersion2             BC|0|||             # Version  0
      BusParBlk$LNG            WM||||              # Parameterblock Length
      # ********

      # Highest Station Adress
          Hsa<Hsa>                 BM|126|2|126|1
      * Highest station address in the network

      # SINEC L2 Adress
          Ts<Ts>                                   BM|1|0|126|1
      * Local SINEC L2 station address

      # Stationtype ( Master or Slave )
          StationType<StationType>         EM|ACTIVE|16|ACTIVE(1)|PASSIVE(0)
      * Master (ACTIVE) or slave (PASSIVE)

      # Baudrate
          BaudRate<BaudRate>       EM|"187.5"|16|"9.6"(0)
                                            |"19.2"(1)
                                            |"93.75"(2)
                                            |"187.5"(3)
                                            |"500.0"(4)
                                            |"1500.0"(7)
                                            |"3000.0"(8)
                                            |"6000.0"(9)
                                            |"12000.0"(10)
                                            |"45.45"(12)
      * SINEC L2 transmission rate

      # ( Parameter ignored ) Medium Redundancy
          MediumRed<MediumRed>           WC|0|||
      * Medium redundancy is not supported

      # Requester Send Retry Counter
          RetryCtr<RetryCtr> WM|1|0|8|1
      * Number of frame repetitions

      # Default Adress, used for Telegram without Adress Extension
          DefaultSap<DefaultSap>               BM|52|0|52|1       [0-2]
      * SAP used for frames without address extension

      # Network Connection Sap
          NetwkConSap<NetwkConSap>                    BM|0|0|255|1
      * SAP used for network interconnections

      # Slot Time
          Tsl<Tsl>                             WM||35|65535|1
      * Receive timeout of sender for acknowledgment of receiver

      # ( Parameter ignored ) Transmitter Fall Time
          Tqui<Tqui>                           WM|0|0|31|1
      * Parameter not required

      # Setup Time
          Tset<Tset>                           WM||0|255|1
      * Minimum time between receiving an acknowledgment
      * and sending a new job 

      # Minimum Station Delay Time
          MinTsdr<MinTsdr>                    WM||0|1023|1
      * The earliest time after which a receiver can send
      * an acknowledgment after receiving a job.

      # Maximum Station Delay Time
          MaxTsdr<MaxTsdr>                    WM||0|1023|1
      * The earliest time after which the sender can send a
      * new job after sending a job. 

      # Target Rotation Time
          Ttr<Ttr>                             DM||35|0xffffffff|1
      * Target rotation time within which the token 
      * must be passed around the logical ring

      # Gap Update Factor
      G<G>         BM||1|100|1
      * After the time "G*TTR" has expired, the gap to the next
      * active station is checked for new stations desiring to enter the ring

      # Always True
      InRingDesired                        WC|1|||
      * The station must always be accepted into the logical ring

      # Always RS485
      PhysLayer                            WC|0|||
      * SINEC L2 supports only RS485

    }

    LSAPListPblk
    {
      # Subblock
      # ********
      SblkNo5                              BC|5|||    # Subblock Number
      SblkVersion5                         BC|0|||    # Version  0
      LSAPListPblk$LNG                     WM||||     # Subblock Length
      # ********

      # List of protected LSAP
          ProtectedLsapList<ProtectedLsapList>   HO||0|126|0
      * List of SAPs reserved for FDL

    }
    
	BusProfile$NAM 					_O||||
	BusProfile
	{
		BusProfileId				BC|30|||
		BusProfileFeatures			WM|0|0|0xffff|1   # default : Alle optionale Blöcke aus
		                                                      # Bit 0=1 : Äquidistanz/Busprofil Block ist vorhanden
		BusProfile$LNG				WM||||

		BusProfileAddData
		{
			BusProfileAddDataId                     BC|0|||           
			BusProfileAddData$LNG                   WM||||
			
			Reserved0					WM|0|||
			Busprofil					BM|0|||
			EquDisMode					BM|0|||
			EquDisFunction				BM|0|||
			NumMasterCl2Allowed			BM|0xff|||
			TokenHoldTime				DM|0|||
			AcycRequestCounter			WM|0xffff|||
			EquDistInterval				DM|0|||
			EquDisMasterAdr				BM|0xff|||
			UF							BM|1|||
			EquDisPassivePause			WM|0|||
			TokenRetryCounter			BM|0|||
			DataRetryCounter			BM|0|||
			PublisherControlInt			DM|10|||
			BscInRing					WM|0|||
			BscOutOfRing				WM|0|||
			PhysicalLayer				BM|0|||
			MediumRedundancy			BM|0|||
			EquDisPulseGroup			BM|0|||
			EquDisPulseCommand			BM|0|||
			EquDisPulseGrFilter			BM|0|||
			MaxActiveStation			BM|0|0|0x7e|
			SynSapEquDis				BM|58|||
			SynSduLenEquDis				BM|2|||
			BusProfileSendInt			WM|0xffff|||
			TDE							DM|0|||
			TM							DM|0|||
			ASPC2EDebugContr1			WM|0|||
			ASPC2EDebugContr2			WM|0|||
			Tjitter						BM|10|||
			Reserved1					HM|0h0000000000000000000000|11|11|11
		}
	}
  }  
}


# ****************************************************************************
