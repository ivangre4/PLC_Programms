			                                                                # Bit 9 : StatusAlarm required
			                                                                # Bit 10 : UpdateAlarm supported
			                                                                # Bit 11 : UpdateAlarm required
			                                                                # Bit 12 : ManufactorAlarm supported
			                                                                # Bit 13 : ManufactorAlarm required
			                                                                # Bit 14 : reserved
			                                                                # Bit 15 : reserved
			          
			          Reserved                          DM|0|0|0xffffffff|1 # Reserviert
			        }
			      }
                }

                # DP Slave Para Data
                SlvParData                                  
                        {
                        # Length of Slave Para Data
                        SlvParData$LNG   WM||||
                                      
                        Reserved1        IC|0b000|3|3|3

                        # Watchdog On = 1 / Watchdog Off = 0
