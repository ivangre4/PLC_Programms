			# ---------------------------------------------------
			# Header
			# ---------------------------------------------------

			# Länge SDB Identblock
			SDBIdentblock$LNG				BM||||

			# Parameterblockkennung ID SDB Kennung
			SDBIdentblock_ID				BC|0x01|||

			# ---------------------------------------------------
			# decodierungsrelevante Information	( Upload )
			# ---------------------------------------------------

			# Baugruppentyp
			Baugruppentyp			SM||||

			# Inhalt des Registry Eintrages "ContentPattern"
			SDB_Inhalt				SM|0|||

			# ---------------------------------------------------
			# Übertragungsrelevante Information
			# ---------------------------------------------------

			# Übertragungsattribut
			Uebertragungsattribut	BM|3|||

			# reserviert
			SDB_KENNUNG_RESV		BM|0|||

			# K-Bus/Partyline
			TSAP_ID_TYP				BM|0|||

			# K-Bus, ohne Bedeutung
			DEVICE_ID				BM|0|||

			# PG-Verbindung
			TSAP_ID1				BM|0xfe|||

			# reserviert
			TSAP_ID2				BM|0|||

			# reserviert
			NEXT_STADDR				BM|0|||

			# Rack/Steckplatz
			REM_STADDR				BM|0|||
		}

