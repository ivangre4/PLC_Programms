			# die Applikationsinformation
			# ---------------------------------------------------

			# Länge des Applikationsnamens
			ApplName$LNG			BM||||

			# Applikationsname ( maximale Länge 255 Zeichen )
			ApplName				AM|""|0|255|0
		}

