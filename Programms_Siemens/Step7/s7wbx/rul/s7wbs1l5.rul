			{
				PBL_PRIV3_ID			WC|3|||
				PBL_PRIV3$LNG			WM||||
				CNT_REPEATER			DM||||	# ATT_CNT_REPEATER_L
				CU_WIRE_LENGTH			DM||||	# ATT_CU_WIRE_LENGTH_L
				LWL_WIRE_LENGTH			DM||||	# ATT_LWL_WIRE_LENGTH_L
				CNT_OLM				DM||||	# ATT_CNT_OLM_L
			}
			
			PBL_MPI_PRIV4$NAM	_O||||
