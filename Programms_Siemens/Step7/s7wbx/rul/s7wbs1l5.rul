			SLAVE_ADR$NAM						_A||||
			SLAVE_ADR
			{
				L2_ADR							BM||||	
			}
		}
		
		PBL_BGR_NAME$NAM			_O||||			# Name und Object-ID der Baugruppe
		PBL_BGR_NAME
		{
			PBL_ID								BC|0x15|||
			PBL_ID_VERSION						BC|0|||
			PBL_BGR_NAME$LNG					WM||||
			
			OBJ_ID								DM||||
			BGR_NAME$LNG						BM||||
			BGR_NAME							AO||0|255|0
		}

		PBL_OPC_FMS_VFD$NAM		_O||||
