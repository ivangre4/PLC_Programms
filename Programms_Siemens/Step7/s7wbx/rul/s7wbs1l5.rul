			WATCHDOG_TIMEOUT					DM|6000|||
			TIMEOUT								DM|15000|||
			DIAG_TIMEOUT						WM|500|||
			
			MODE								WM|0|||   # 0 = DP-Lib   1 = DP-Base
			
			VMDID_APP							DM||||
		}
		
		PBL_DP_MASTER_HW_EVENT$NAM				_O||||
		PBL_DP_MASTER_HW_EVENT
		{
			PBL_ID								BC|0x0F|||
			PBL_ID_VERSION						BC|0|||
