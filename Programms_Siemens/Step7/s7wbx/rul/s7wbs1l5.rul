			PBL_ID								BC|0x10|||
			PBL_ID_VERSION						BC|0|||
			FO_CONNECTOR_PBLK$LNG					WM||||

			CONNECTOR							WM|0|0|1|1
		}
		
		UHRZEIT_PBLK$NAM		_O||||			# Konfigurieren von Optischen, bzw. elektrischen Anschluß für DP-Master
		UHRZEIT_PBLK
		{
			PBL_ID								BC|0x11|||
			PBL_ID_VERSION						BC|0|||
			UHRZEIT_PBLK$LNG					WM||||

			MODE								WM|0|0|2|1
		}
		
		
		PBL_DP_MASTER_OPC$NAM					_O||||
