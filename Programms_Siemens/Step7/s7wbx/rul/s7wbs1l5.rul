		{
			PBL_DPC2_ID			BC|0x42|||
			PBL_DPC2_VERSION	BC|0|||
			PBL_DPC2$LNG		WM||||
			
			IS_CP5613_14		BM|0|0|1|1
			USE_DPC2			BM|0|0|2|1
			USE_DPC2_CY			BM|0|0|1|1
			
			DPC2_TIMEOUT			DM|15000|0|100000|1
			DPC2_TIMEOUT_CONNECT	DM|15000|0|100000|1
		}
		
